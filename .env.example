# RPC Endpoint (use a paid RPC for better performance)
RPC_ENDPOINT=https://api.mainnet-beta.solana.com

# Target CA to monitor (the token you want to watch for buys)
TARGET_CA=your_token_contract_address_here

# Wallet private keys for rotation (base58 encoded)
# Add as many as you want: WALLET_1, WALLET_2, WALLET_3, etc.
WALLET_1=your_first_wallet_private_key
WALLET_2=your_second_wallet_private_key
WALLET_3=your_third_wallet_private_key

# Or use a single wallet
# PRIVATE_KEY=your_wallet_private_key

# Token launch settings
TOKEN_NAME=Annoying Coin
TOKEN_SYMBOL=ANNOY
DEV_BUY_SOL=0.01
PRIORITY_FEE=0.0005

# Cooldown between launches (milliseconds)
LAUNCH_COOLDOWN_MS=30000

# Social links (optional)
TWITTER_URL=https://twitter.com/annoyingcoin
TELEGRAM_URL=
WEBSITE_URL=

# Server port
PORT=3000
